#include <iostream> // Подключаем библиотеку iostream, которая позволяет использовать ввод и вывод данных.
#include <set> // Подключаем библиотеку set, которая позволяет использовать структуру данных "множество".
using namespace std; // Используем пространство имен std, чтобы не писать std:: перед каждым вызовом функции из этого пространства имен.
 
int main() { // Начало основной функции программы.
   int N; // Объявляем переменную N для хранения количества чисел.
   cin >> N; // Считываем значение N из входных данных.
   set<int> numbers; // Объявляем множество чисел.
   for (int i = 0; i < N; i++) { // Запускаем цикл от 0 до N.
       int num; // Объявляем переменную num для хранения текущего числа.
       cin >> num; // Считываем текущее число из входных данных.
       numbers.insert(num); // Добавляем текущее число в множество.
   }
   cout << numbers.size() << endl; // Выводим размер множества, то есть количество уникальных чисел.
   return 0; // Завершаем функцию main и возвращаем 0, что означает успешное выполнение программы.
}
