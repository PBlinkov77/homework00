#include <iostream> // Подключаем библиотеку iostream, которая позволяет использовать ввод и вывод данных.
#include <deque> // Подключаем библиотеку deque, которая позволяет использовать структуру данных "двусторонняя очередь".
using namespace std; // Используем пространство имен std, чтобы не писать std:: перед каждым вызовом функции из этого пространства имен.
 
bool isCorrect(string s) { // Объявляем функцию isCorrect, которая принимает строку скобок и возвращает true, если последовательность скобок правильная, и false в противном случае.
   stack<char> st; // Объявляем стек для хранения открывающих скобок.
   for (char c : s) { // Запускаем цикл для каждого символа в строке.
       if (c == '(' || c == '[' || c == '{') { // Если символ является открывающей скобкой...
           st.push(c); // ...то добавляем его в стек.
       } else { // Если символ является закрывающей скобкой...
           if (st.empty()) { // ...то проверяем, пуст ли стек. Если стек пуст, то последовательность скобок неправильная.
               return false;
           }
           char top = st.top(); // Берем верхний элемент стека.
           st.pop(); // Удаляем верхний элемент стека.
           if (c == ')' && top != '(') { // Если текущий символ - это закрывающая круглая скобка, а верхний элемент стека - не открывающая круглая скобка, то последовательность скобок неправильная.
               return false;
           }
           if (c == ']' && top != '[') { // Аналогично для квадратных скобок.
               return false;
           }
           if (c == '}' && top != '{') { // Аналогично для фигурных скобок.
               return false;
           }
       }
   }
   return st.empty(); // Если после обработки всех символов стек пуст, то последовательность скобок правильная. В противном случае - неправильная.
}
 
int main() { // Начало основной функции программы.
   string s; // Объявляем строковую переменную для хранения скобочной последовательности.
   cin >> s; // Считываем скобочную последовательность из входных данных.
   if (isCorrect(s)) { // Если последовательность скобок правильная...
       cout << "yes"; // ...то выводим "yes".
   } else { // Если последовательность скобок неправильная...
       cout << "no"; // ...то выводим "no".
   }
   return 0; // Завершаем функцию main и возвращаем 0, что означает успешное выполнение программы.
}
